{
  "name": "prindicator",
  "displayName": "PR Indicator",
  "description": "Notifications for Pull Requests",
  "icon": "media/prindicator.png",
  "repository": {
    "url": "https://github.com/joelmagner/vsprindicator"
  },
  "publisher": "MagnerJoel",
  "version": "0.0.6",
  "engines": {
    "vscode": "^1.54.0"
  },
  "categories": [
    "Other"
  ],
  "extensionDependencies": [
    "vscode.git"
  ],
  "activationEvents": [
    "onCommand:prindicator.authenticateGithub",
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "prindicator.authenticateGithub",
        "title": "Authenticate To Github",
        "category": "PR Indicator"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "test-compile": "tsc -p ./",
    "test-watch": "tsc -watch -p ./",
    "pretest": "npm run test-compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@octokit/types": "^6.16.4",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/passport": "^1.0.6",
    "@types/passport-github": "^1.1.5",
    "@types/vscode": "^1.54.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "passport": "^0.4.1",
    "passport-github": "^1.1.0",
    "ts-loader": "^8.0.14",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0",
    "webpack": "^5.19.0",
    "webpack-cli": "^4.4.0"
  },
  "dependencies": {
    "@octokit/rest": "^18.3.5"
  }
}
